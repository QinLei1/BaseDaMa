<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <gradient
                android:startColor="#b7b5b8"
                android:centerColor="#b7b5b8"
                android:endColor="#b7b5b8"

                />
        </shape>
    </item>

    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <gradient
                    android:startColor="#feea07"
                    android:centerColor="#feea07"
                    android:endColor="#feea07"
                    />
            </shape>
        </clip>
    </item>

    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient
                    android:startColor="#feea07"
                    android:centerColor="#feea07"
                    android:endColor="#feea07"
                    />
            </shape>
        </clip>
    </item>

</layer-list>
